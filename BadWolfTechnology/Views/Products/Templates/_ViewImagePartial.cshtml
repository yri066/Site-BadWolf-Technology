@model (IList<ContentItem> items, int contentNumber, string folderName)
@*
    For more information on enabling MVC for empty projects, visit https://go.microsoft.com/fwlink/?LinkID=397860
*@
@{
}

@foreach (var item in Model.items)
{
    @if (item.ImageName != null)
    {
        <a class="newsUrl" href="~/image/@Model.folderName/@item.ImageName" data-fancybox="image">
            <img class="wrapper exmp" loading="lazy" src="~/image/@Model.folderName/@item.ImageName" />
        </a>
        <br />
    }
}
